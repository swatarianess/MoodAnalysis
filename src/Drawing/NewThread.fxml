<AnchorPane id="AnchorPane" fx:id="newThreadAnchorPane" accessibleText="New Thread" prefHeight="160.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Drawing.Controller.NewThreadController">

   <Label layoutX="28.0" layoutY="40.0" text="Website to add thread:" />
   <Label layoutX="27.0" layoutY="78.0" text="Thread to add:" />

   <TextField fx:id="addThreadTextField" layoutX="215.0" layoutY="74.0" prefHeight="25.0" prefWidth="180.0" promptText="Thread" />

   <ChoiceBox fx:id="websitesChoiceBox" layoutX="216.0" layoutY="36.0" prefHeight="25.0" prefWidth="180.0" />
   <Button fx:id="cancelButton" layoutX="216.0" layoutY="118.0" mnemonicParsing="false" text="Cancel" />
   <Button fx:id="addButton" layoutX="306.0" layoutY="118.0" mnemonicParsing="false" onAction="#handleAddThread" text="Add" />
</AnchorPane>
